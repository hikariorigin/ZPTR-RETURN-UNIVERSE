# ğŸ§© Earth Engine v5 â€“ Foundation Code

ä»¥ä¸‹ã¯ Earth Engine v5 ã®ç‚‰å¿ƒã‚³ãƒ¼ãƒ‰ã€‚
ãã®ã¾ã¾ Python ã§å‹•ãã€‚

import numpy as np
import matplotlib.pyplot as plt
from mpl_toolkits.mplot3d import Axes3D

# ======================================================
# 0. Utility
# ======================================================
def sph_to_cart(r, theta, phi):
    x = r * np.sin(theta) * np.cos(phi)
    y = r * np.sin(theta) * np.sin(phi)
    z = r * np.cos(theta)
    return x, y, z


# ======================================================
# 1. Earth Sphere Setup
# ======================================================
lat_res = 180
lon_res = 360

lat = np.linspace(-90, 90, lat_res)
lon = np.linspace(-180, 180, lon_res)

LAT, LON = np.meshgrid(lat, lon)

theta = np.radians(90 - LAT)   # colatitude
phi   = np.radians(LON)

R = 1.0
X, Y, Z = sph_to_cart(R, theta, phi)


# ======================================================
# 2. Geomagnetic Dipole
# ======================================================
dipole = np.cos(theta)


# ======================================================
# 3. Ï„-field fold (3 + 5 + 6)
# ======================================================
N = 400
theta1d = np.linspace(-np.pi, np.pi, N)
tau_base = np.sin(3 * theta1d) + 0.4 * np.cos(5 * theta1d)

a = 1.0
b = 0.42
c = 0.18

tau_interp = np.interp(phi, theta1d, tau_base)

tau_field = (
      a * tau_interp
    + b * np.sin(theta)
    + c * np.cos(2 * phi)
)


# ======================================================
# 4. ECDO Oscillation (Coreâ€“Mantle Decoupling)
# ======================================================
ECDO = 0.12 * np.cos(5 * theta + 2 * phi)

Earth_core_wave = dipole + 0.35 * tau_field + ECDO


# ======================================================
# 5. Base Map Visualization
# ======================================================
fig = plt.figure(figsize=(12, 12))
ax = fig.add_subplot(111, projection='3d')

surf = ax.plot_surface(
    X, Y, Z,
    facecolors=plt.cm.inferno(
        (Earth_core_wave - Earth_core_wave.min()) /
        (Earth_core_wave.max() - Earth_core_wave.min() + 1e-9)
    ),
    rstride=2, cstride=2,
    linewidth=0, antialiased=False, shade=False
)

ax.set_box_aspect([1, 1, 1])
ax.set_axis_off()
ax.set_title("ZPTR Earth Engine v5 â€“ Base Field", fontsize=16)

plt.show()


# ======================================================
# 6. Node Registration API
# ======================================================
def register_node(lat, lon, label="node"):
    t = np.radians(90 - lat)
    p = np.radians(lon)
    x, y, z = sph_to_cart(1.02, t, p)
    return {"label": label, "xyz": (x, y, z), "t": t, "p": p}


